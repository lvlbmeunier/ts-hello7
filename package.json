{
  "name": "ts-service-boilerplate",
  "version": "0.0.1",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "LVL Studios",
  "license": "MIT",
  "scripts": {
    "start": "node dist/server.js",
    "build": "npm run tslint && rm -rf ./dist && npm run tsc && node ./assetCopy.js",
    "watch": "nodemon dist/server.js",
    "test": "node ./assetCopy.js && jest --forceExit --coverage --verbose",
    "watch-ts": "tsc -w",
    "tsc": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-basic-auth": "^1.1.5",
    "lodash": "^4.17.4",
    "microframework-w3tec": "^0.6.3",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "routing-controllers": "^0.7.7",
    "swagger-ui-express": "^3.0.10",
    "typedi": "^0.8.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.1.3",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.91",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.10.25",
    "@types/request": "^2.47.0",
    "@types/supertest": "^2.0.4",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "jest": "^22.0.4",
    "nodemon": "^1.13.0",
    "prettier": "^1.14.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
